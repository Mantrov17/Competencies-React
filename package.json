{
  "name": "webpack",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && node server/index.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:watch": "cross-env NODE_ENV=development node ./node_modules/webpack/bin/webpack.js --config webpack.config.js --watch",
    "server:users": "json-server -w src/data/users.json -p 3001",
    "server:hardSkills": "json-server -w src/data/hardSkills.json -p 3002",
    "server:softSkills": "json-server -w src/data/softSkills.json -p 3003",
    "devs": "concurrently \"npm run server:users\" \"npm run server:hardSkills\" \"npm run server:softSkills\" \"npm run dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "body-parser": "^1.19.1",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.1.0",
    "express": "^4.17.2",
    "json-server": "^1.0.0-beta.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.2",
    "mini-css-extract-plugin": "^2.9.1",
    "sass": "^1.45.1",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
